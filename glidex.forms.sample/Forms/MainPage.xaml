<?xml version="1.0" encoding="utf-8" ?>
<ContentPage 
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="Android.Glide.Sample.MainPage"
    Title="GlideX.Forms Sample">
    <ListView ItemSelected="OnItemSelected">
        <ListView.ItemsSource>
            <x:Array Type="{x:Type x:String}">
                <x:String>Grid Example</x:String>
                <x:String>Edge Cases</x:String>
                <x:String>ViewCells</x:String>
                <x:String>ImageCells</x:String>
                <x:String>Huge Images</x:String>
                <x:String>Toggle Images</x:String>
                <x:String>Toggle Images Material</x:String>
                <x:String>Images Should Match</x:String>
                <x:String>ListView Of Doom</x:String>
                <x:String>CollectionView Of Doom</x:String>
                <x:String>FlexLayout Image Buttons</x:String>
                <x:String>VisualStateManager</x:String>
            </x:Array>
        </ListView.ItemsSource>
    </ListView>
</ContentPage>
